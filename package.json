{
  "name": "redis-lua2js",
  "version": "3.0.0",
  "description": "Convert a redis lua script into a simple node module",
  "main": "dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --cache .",
    "test": "mocha",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/redis-lua2js.git"
  },
  "keywords": [
    "redis",
    "lua",
    "ioredis"
  ],
  "author": "Julian Grinblat <julian@dotcore.co.il>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotcore64/redis-lua2js/issues"
  },
  "homepage": "https://github.com/dotcore64/redis-lua2js#readme",
  "devDependencies": {
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "dirname-filename-esm": "^1.1.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.4",
    "module-from-string": "^3.1.4",
    "rollup": "^2.64.0"
  },
  "dependencies": {
    "astring": "^1.8.1"
  }
}
